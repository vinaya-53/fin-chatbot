{
    "version": 2,
   "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/app.py" }
  ],  "framework": "vercel",
  "dependencies": {
    "requirements.txt": ["pip", "install", "-r", "requirements.txt"] 
  }
  }
  